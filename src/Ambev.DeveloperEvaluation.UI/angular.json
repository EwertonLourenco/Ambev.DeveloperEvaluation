{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "Ambev.DeveloperEvaluation.UI": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/Ambev.DeveloperEvaluation.UI",
            "browser": "src/main.ts",
            "index": "src/index.html",
            "polyfills": [ "zone.js" ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [ "src/styles.css" ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB"
                }
              ],
              "outputHashing": "all",
              "optimization": true
            },
            "development": { "optimization": false }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "port": 4200 },
          "configurations": {
            "production": { "buildTarget": "Ambev.DeveloperEvaluation.UI:build:production" },
            "development": { "buildTarget": "Ambev.DeveloperEvaluation.UI:build:development" }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": "1742f142-6749-4f60-9ad0-c0033cb3ec42"
  }
}
